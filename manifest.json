{
"manifest_version": 2,
    "name": "Reddit Churning Acronyms",
    "description": "Replaces common churning acronyms with their actual meaning",
    "version": "1.0",
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
    "content_scripts": [
        {
            "matches": [
                "*://www.reddit.com/r/churning/c*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}